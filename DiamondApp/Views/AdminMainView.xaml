                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFF6F6F6" Offset="0.25"/>
                    <GradientStop Color="#FFEAE8E8" Offset="0.25"/>
                    <GradientStop Color="#FFDCD9D9" Offset="0.8"/>
                    <GradientStop Color="#FF308AB4" Offset="1"/>
                </LinearGradientBrush>
            </Menu.Background>
            <MenuItem Header="Menu" HorizontalAlignment="Left" Width="47">
                <MenuItem Header="Dodaj" HorizontalAlignment="Left" Width="167" Margin="0,0,0,0" Command="{Binding CreateNewPropositionCommand}"/>
                <MenuItem Header="Edytuj" HorizontalAlignment="Left" Width="167" Margin="0,0,0,0" Command="{Binding ChangePropositionCommand }"/>
                <MenuItem Header="Lista propozycji" HorizontalAlignment="Left"  Margin="0" Command="{Binding ShowPropositionsCommand}"/>
                <MenuItem Header="Zakończ" HorizontalAlignment="Left" Width="167"  Margin="0,0,0,0"/>
            </MenuItem>
            <MenuItem Header="Edytuj słownik" Command="{Binding EditDictionaryCommand}"/>
            <MenuItem Header="Użytkownicy">
                <MenuItem Header="Dodaj" Command="{Binding AddUserCommand}"/>
                <MenuItem Header="Edytuj" Command="{Binding EditUserCommand}" />
                <MenuItem Header="Resetuj hasło" Command="{Binding ResetPasswordCommand}"/>
                <MenuItem Header="Usuń" Command="{Binding RemoveUserCommand}"/>

            </MenuItem>
        </Menu>
