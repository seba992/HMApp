                    <GridViewColumn Width="200" Header="Data aktualizacji" DisplayMemberBinding="{Binding UpdateDate}" />
                    <GridViewColumn Width="70" Header="Status" DisplayMemberBinding="{Binding Status}" />

                    <GridViewColumn Header="Stan">
                        <GridViewColumn.CellTemplate>
                            <DataTemplate>
                                <ComboBox  Width="150">
                                    <ComboBoxItem IsSelected="True" Content="Nierozpatrzona"/>
                                    <ComboBoxItem Content="Gotowa" />
                                </ComboBox>
                            </DataTemplate>
                        </GridViewColumn.CellTemplate>
                    </GridViewColumn>
                    <GridViewColumn>
                        <GridViewColumn.CellTemplate>
                            <DataTemplate>
                                <Button Content="Edytuj" Padding="5,2"/>
                            </DataTemplate>
                        </GridViewColumn.CellTemplate>
                    </GridViewColumn>
                </GridView>
            </ListView.View>
